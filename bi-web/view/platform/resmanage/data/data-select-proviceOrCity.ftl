<span class="f14"><@spring.message code="data.provice.selectProviceOrCity"/></span>
<div class="select-info" id="selectInfo">
	<ul>
		<li class="form-group form-group-sm">
			<div class="label-class labelWidth">
				<span class="f14"><@spring.message code="data.provice.provice"/></span>
			</div>
			<div class="input-class selectWidth">
				<select class="form-control" data-placeholder="<@spring.message code="data.provice.selectProvice"/>" id="provice">
				</select>
			</div>
		</li>
		<li class="form-group form-group-sm">
			<div class="label-class labelWidth ">
				<span class="f14"><@spring.message code="data.provice.city"/></span>
			</div>
			<div class="input-class selectWidth">
				<select class="form-control" data-placeholder="<@spring.message code="data.provice.selectCity"/>" id="city">
				</select>
			</div>
		</li>
	</ul>
</div>
